      "em": "readonly",
      "embed": "readonly",
      "fieldset": "readonly",
      "figcaption": "readonly",
      "figure": "readonly",
      "footer": "readonly",
      "form": "readonly",
      "fragment": "readonly",
      "h1": "readonly",
      "h2": "readonly",
      "h3": "readonly",
