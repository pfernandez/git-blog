{
  "name": "expressive",
  "version": "0.0.1",
  "description": "A Lisp syntax for JavaScript",
  "author": "Paul Fernandez",
  "license": "ISC",
  "type": "module",
  "keywords": [
    "javascript",
    "lisp"
  ],
  "homepage": "https://github.com/pfernandez/expressive#readme",
  "bugs": {
    "url": "https://github.com/pfernandez/expressive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfernandez/expressive.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "nodemon --ignore src"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.14"
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": { "browser": true, "es2021": true },

    "rules": {
      "array-bracket-newline": ["warn", "consistent"],
      "arrow-parens": ["warn", "as-needed"],
      "arrow-spacing": "warn",
      "comma-spacing": ["warn", { "before": false, "after": true }],
      "comma-style": ["warn", "last"],
      "computed-property-spacing": ["warn", "never"],
      "eol-last": ["warn", "always"],
      "indent": ["warn", 2, {
        "ImportDeclaration": "first",
        "ObjectExpression": "first",
        "VariableDeclarator": { "var": 2, "let": 2, "const": 3 }
      }],
      "key-spacing": [ "warn", { "mode": "minimum" } ],
      "linebreak-style": ["error", "unix"],
      "no-extra-parens": "warn",
      "no-multiple-empty-lines": ["warn", { "max": 2, "maxEOF": 1 }],
      "no-multi-spaces": ["warn", {
        "ignoreEOLComments": true,
        "exceptions": { "Property": false }
      }],
      "no-trailing-spaces": "warn",
      "no-undef": "warn",
      "no-unused-vars": ["warn", {
        "args": "all",
        "argsIgnorePattern": "^_"
      }],
      "object-curly-newline": ["warn", { "consistent": true }],
      "object-curly-spacing": ["warn", "always"],
      "operator-linebreak": ["warn", "before", {
        "overrides": { "=": "ignore" }
      }],
      "prefer-rest-params": "warn",
      "quote-props": ["warn", "as-needed"],
      "quotes": ["warn", "single"],
      "semi": ["warn", "never"],
      "sort-imports": "warn",
      "space-before-function-paren": ["warn", {
        "anonymous": "never",
        "asyncArrow": "always",
        "named": "never"
      }],
      "spaced-comment": ["warn", "always"],
      "space-in-parens": "warn",
      "space-infix-ops": "warn"
    },

    "globals": {
      "apply": "readonly",
      "bool": "readonly",
      "deepMap": "readonly",
      "each": "readonly",
      "entries": "readonly",
      "eq": "readonly",
      "evaluate": "readonly",
      "exists": "readonly",
      "filter": "readonly",
      "first": "readonly",
      "globalize": "readonly",
      "identity": "readonly",
      "instance": "readonly",
      "isArray": "readonly",
      "isFunction": "readonly",
      "join": "readonly",
      "keys": "readonly",
      "last": "readonly",
      "length": "readonly",
      "log": "readonly",
      "omap": "readonly",
      "omit": "readonly",
      "partial": "readonly",
      "push": "readonly",
      "rest": "readonly",
      "reverse": "readonly",
      "slice": "readonly",
      "split": "readonly",
      "sum": "readonly",
      "type": "readonly",
      "walk": "readonly",

      "a": "readonly",
      "abbr": "readonly",
      "address": "readonly",
      "area": "readonly",
      "article": "readonly",
      "aside": "readonly",
      "audio": "readonly",
      "b": "readonly",
